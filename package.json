{
  "name": "Android_Messages",
  "version": "1.0.0",
  "description": "An electron container for the Android Messages Webapp",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds",
    "package-win": "electron-packager . \"Messages for web\" --overwrite --asar=true --platform=win32 --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Messages for web\"",
    "package-linux": "electron-packager .  \"Messages for web\" --overwrite --asar=true --platform=linux --arch=x64 --prune=true --out=release-builds"
  },
  "repository": "https://github.com/yokoleia/Android_Messages",
  "keywords": [
    "Android",
    "Messages",
    "start",
    "Electron",
    "First Project"
  ],
  "author": "yokoleia",
  "license": "UNLICENSED",
  "devDependencies": {
    "electron": "^2.0.3",
    "electron-packager": "^12.1.0"
  }
}
